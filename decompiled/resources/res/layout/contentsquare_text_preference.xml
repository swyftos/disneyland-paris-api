<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:textAppearance="@style/contentsquare_AppTheme.TextAppearance.BodyText"
        android:textColor="@color/contentsquare_black"
        android:id="@+id/contentsquare_preference_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/contentsquare_preference_summary"
        app:layout_constraintTop_toTopOf="parent"/>
    <TextView
        android:id="@+id/contentsquare_preference_summary"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/contentsquare_preference_title"/>
</merge>
